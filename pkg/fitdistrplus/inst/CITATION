citHeader("To cite fitdistrplus in publications use:")


citEntry(entry="Manual",
         title = "fitdistrplus: tools to fit parametric distributions",
         author = "Marie Laure Delignette-Muller",
	 year = 2009,
         note = "R package version 0.1-1",

         textVersion =
	 paste(	"M. L. Delignette-Muller",
		"(2009),",
	       	"fitdistrplus: tools to fit parametric distributions.")
)

