\end_layout

\begin_layout Standard

   boot <- matrix(rbinom(length(datDR$dose)*n,datDR$ni,ml),nrow=length(datDR$dos
e))
\end_layout

\begin_layout Standard

   apply(boot,2,estDR,ref=datDR)}
